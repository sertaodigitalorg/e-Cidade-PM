{
    "name": "ecidade/data-base-library",
    "description": "Biblioteca centralizada de entidades, repositórios e migrações do e-Cidade.",
    "type": "symfony-bundle",
    "require": {
        "doctrine/orm": "^3.3",
        "symfony/orm-pack": "^2.4",
        "symfony/security-core": "^7.2",
        "symfony/validator": "^7.2",
        "symfony/framework-bundle": "7.2.*",
        "doctrine/doctrine-bundle": "^2.14.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "ECidade\\DataBaseLibrary\\": "src/"
        }
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "dont-include-dependencies": true
        }
    },
    "authors": [
        {
            "name": "Wellington Carvalho",
            "email": "wellington.cs@hotmail.com"
        }
    ],
    "minimum-stability": "dev"
}
